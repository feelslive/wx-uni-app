<view class="order-card"><view class="order-card-top border_solid"><view class="order-card-top-yime" style="font-weight:bold;">{{item.service_time}}</view><view class="order-card-top-status">{{item.status_name}}</view></view><view class="order-card-center border_solid"><view class="order-card-center-title"><text>{{item.product_name+"· "+item.used_time+item.used_unit}}</text></view><view class="order-card-center-info"><van-icon vue-id="3f61ecd2-1" name="manager" color="#DCDCDC" size="28rpx" data-com-type="wx" bind:__l="__l"></van-icon><view class="order-card-center-text">{{item.user_name+" "+item.user_mobile}}</view></view><view class="order-card-center-info"><van-icon vue-id="3f61ecd2-2" name="location" color="#DCDCDC" size="28rpx" data-com-type="wx" bind:__l="__l"></van-icon><view class="order-card-center-text">{{item.address}}</view></view></view><view class="order-card-button"><view class="order-card-button-left">{{item.service_user_name}}</view><view class="order-card-button-right"><van-button vue-id="3f61ecd2-3" custom-class="modify" plain="{{true}}" color="{{themeColor}}" data-phone="{{item.store_mobile}}" round="{{true}}" data-event-opts="{{[['^tap',[['onPhone']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}">修改时间</van-button></view></view><van-dialog vue-id="3f61ecd2-4" id="van-dialog" data-com-type="wx" bind:__l="__l"></van-dialog></view>