<block wx:if="{{result.pay_actual}}"><view class="order-detail"><view class="order-price"><view class="order-price-key">{{''+result.order_status_name+'：'}}</view><view class="order-price-val">{{'¥'+result.pay_actual+''}}</view></view><block wx:if="{{result.address}}"><view class="order-card"><view class="order-card-info"><view class="order-card-icon"><van-icon vue-id="28dee4c1-1" name="location-o" color="{{themeColor}}" size="36rpx" data-com-type="wx" bind:__l="__l"></van-icon></view><view class="order-card-right"><view class="order-card-right-name">{{''+result.user_name+" "+result.user_mobile+''}}</view><view class="order-card-right-site">{{''+result.address+''}}</view></view></view></view></block><block wx:if="{{result.product_name}}"><view class="order-card"><view class="order-card-title border_solid">项目信息</view><view class="order-card-info"><view class="order-card-left">项目名称</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{''+result.product_name+''}}</view></view></view></view><view class="order-card-info"><view class="order-card-left">数量</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{''+result.product_unit+"\n                        "+result.product_unit_name+''}}</view><view class="order-card-right-tag">{{result.product_type_name}}</view><view class="order-card-right-tag">{{result.service_type}}</view></view></view></view><block wx:if="{{result.product_type=='2'&&result.service_expire_time}}"><view class="order-card-info"><view class="order-card-left">有效期</view><view class="order-card-right"><view class="order-card-right-vals"><view style="display:flex;align-items:center;"><text>{{result.service_start_time}}</text><text>{{"至"+result.service_expire_time}}</text></view></view></view></view></block></view></block><block wx:if="{{result.pay_actual||result.pay_money}}"><view class="order-card"><view class="order-card-title border_solid">结算信息</view><view class="order-card-info"><view class="order-card-left">现价</view><view class="order-card-right">{{''+(result.pay_money==''?'无':'¥'+result.pay_money)+''}}</view></view><view class="order-card-info"><view class="order-card-left">优惠券</view><block wx:if="{{result.coupon_limit==='0.00'}}"><view class="order-card-right"><text>无</text></view></block><block wx:else><view class="order-card-right">{{''+result.coupon_type+" - ¥"+result.coupon_limit+''}}</view></block></view><view class="order-card-info"><view class="order-card-left">支付备注</view><view class="order-card-right">{{''+(result.pay_remark==''?'无':result.pay_remark)+''}}</view></view><view class="order-card-info"><view class="order-card-left">购买金额</view><view class="order-card-right">{{'¥'+result.pay_actual+''}}</view></view></view></block><block wx:if="{{result.order_number}}"><view class="order-card"><view class="order-card-title border_solid">其他</view><view class="order-card-info"><view class="order-card-left">购买时间</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.create_time}}</view></view></view></view><view class="order-card-info"><view class="order-card-left">支付方式</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.order_status=='1'?'无':result.pay_type}}</view></view></view></view><view class="order-card-info"><view class="order-card-left">支付时间</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.order_status=='1'?'无':result.pay_time}}</view></view></view></view><view class="order-card-info"><view class="order-card-left">订单编号</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.order_number}}</view><view class="order-card-right-func" data-id="{{result.order_number}}" data-event-opts="{{[['tap',[['onCopy',['$event']]]]]}}" bindtap="__e"><view class="order-card-right-func-text" style="margin-left:20rpx;">复制</view></view></view></view></view><view class="order-card-info"><view class="order-card-left">购买门店</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.store_name}}</view></view></view></view><view class="order-card-info"><view class="order-card-left">门店客服</view><view class="order-card-right"><view class="order-card-right-vals"><view>{{result.customer_mobile}}</view><view data-event-opts="{{[['tap',[['makePhone',['$event']]]]]}}" class="order-card-right-func" bindtap="__e"><van-icon style="margin-left:20rpx;" vue-id="28dee4c1-2" name="phone-o" size="34rpx" data-com-type="wx" bind:__l="__l"></van-icon><view class="order-card-right-func-text">联系</view></view></view></view></view></view></block><block wx:if="{{result.order_status=='2'&&result.remaining_time>=1}}"><view class="handle-btns"><van-button vue-id="28dee4c1-3" type="default" data-type="{{result.product_type}}" data-name="{{result.product_name}}" data-id="{{result.order_number}}" data-num="{{result.remaining_time}}" data-unit="{{result.product_unit_name}}" round="{{true}}" data-event-opts="{{[['^click',[['onMakeOrder']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">预约项目</van-button></view></block></view></block>